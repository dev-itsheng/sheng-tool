{
    "name": "sheng-tool",
    "version": "0.0.1",
    "description": "a simple tool",
    "homepage": "https://gitee.com/itsheng/sheng-tool",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "build-test": "ts-node ./script/build-test.ts",
        "test": "jest",
        "clean-before": "rimraf ./dist",
        "build-bundle": "rollup -c",
        "build-typescript-declaration": "tsc -d && api-extractor run",
        "build-document": "ts-node ./script/build-docs.ts",
        "clean-after": "rimraf ./temp-typings && rimraf ./docs",
        "build": "npm run clean-before && npm run build-bundle && npm run build-typescript-declaration && npm run build-document && npm run clean-after"
    },
    "author": "dev-itsheng",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-typescript": "^7.10.1",
        "@microsoft/api-extractor": "^7.13.1",
        "@microsoft/tsdoc": "^0.13.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-node-resolve": "^11.2.0",
        "@types/jest": "^25.2.3",
        "@types/jsdom": "^16.2.3",
        "@types/lodash-es": "^4.17.4",
        "babel-jest": "^26.0.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "jest": "^26.0.1",
        "jsdom": "^16.2.2",
        "lodash-es": "^4.17.21",
        "rimraf": "^3.0.2",
        "rollup": "^2.39.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "ts-node": "^9.1.1",
        "typedoc": "^0.20.28",
        "typescript": "^4.2.2"
    },
    "files": [
        "dist/"
    ]
}
